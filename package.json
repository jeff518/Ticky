{
  "name": "ticky",
  "version": "1.0.0",
  "description": "Enterprise ticket triage helpdesk - Electron desktop app",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && node server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "electron .",
    "seed": "cd backend && node seed.js",
    "postinstall": "cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": ["electron", "ticketing", "helpdesk", "react"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "wait-on": "^7.2.0"
  }
}
